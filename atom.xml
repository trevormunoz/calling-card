---
layout: nil
---
<?xml version="1.0" encoding="UTF-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title>Trevor Muñoz</title>
	<link href="http://trevormunoz.com/atom.xml" rel="self"/>
	<link href="http://trevormunoz.com/"/>
	<updated>{{ site.time | date_to_xmlschema }}></updated>
	<id>http://trevormunoz.com/</id>
	<author>
		<name>Trevor Muñoz</name>
		<email>trevor@trevormunoz.com</email>
	</author>
	
	{% for post in site.posts %}
	<entry>
		<title>{{ post.title }}</title>
		<link href="http://trevormunoz.com{{ post.url }}"/>
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<id>http://trevormunoz.com{{ post.id }}</id>
		<content type="html">{{ post.content | xml_escape }}</content>
	</entry>
	{% endfor %}
	
</feed>